{"ast":null,"code":"export default {\n  data: function data() {\n    return {\n      placeholderText: \"请输入500个字符以内\",\n      input: '',\n      fontSize: 16,\n      centerDialogVisible: false\n    };\n  },\n  methods: {\n    increaseFontSize: function increaseFontSize() {\n      this.fontSize += 2;\n    },\n    decreaseFontSize: function decreaseFontSize() {\n      if (this.fontSize > 8) {\n        // 防止字体太小\n        this.fontSize -= 2;\n      }\n    },\n    sureBtnClick: function sureBtnClick() {\n      if (this.input.length <= 0) {\n        alert(\"请输入内容\");\n        return;\n      }\n      alert(this.input);\n      this.centerDialogVisible = false;\n    },\n    twoSum: function twoSum(nums, target) {\n      for (var i = 0; i < nums.length; i++) {\n        for (var j = 0; j < nums.length; j++) {\n          if (nums[i] + nums[j] === target) {\n            return [i, j];\n          }\n        }\n      }\n      return [-1, -1];\n    }\n  }\n};","map":{"version":3,"names":["data","placeholderText","input","fontSize","centerDialogVisible","methods","increaseFontSize","decreaseFontSize","sureBtnClick","length","alert","twoSum","nums","target","i","j"],"sources":["src/views/testPage/Test_1.vue"],"sourcesContent":["<template>\n  <div class=\"div_box\">\n    <div>{{ twoSum([2, 3, 4, 5], 9)) }} </div>\n    <el-button type=\"primary\" round @click=\"centerDialogVisible = true\">打开Dialog</el-button>\n\n    <el-dialog title=\"提示\" :visible.sync=\"centerDialogVisible\" width=\"50%\" center>\n\n      <el-form>\n        <el-form-item>\n          <el-input type=\"textarea\" :style=\"{ fontSize: fontSize + 'px' }\" v-model=\"input\"\n            :placeholder=\"placeholderText\" :autosize=\"{ minRows: 3, maxRows: 5 }\">\n          </el-input>\n          <!-- <el-button size='mini' type=\"primary\" round @click=\"increaseFontSize\">增大字体</el-button> -->\n          <!-- <el-button size='mini' type=\"success\" round @click=\"decreaseFontSize\">减小字体</el-button> -->\n        </el-form-item>\n      </el-form>\n\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"centerDialogVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"sureBtnClick\" style=\"margin-left:60px\">确 定</el-button>\n      </span>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      placeholderText: \"请输入500个字符以内\",\n      input: '',\n      fontSize: 16,\n      centerDialogVisible: false,\n    };\n  },\n  methods: {\n    increaseFontSize() {\n      this.fontSize += 2;\n    },\n    decreaseFontSize() {\n      if (this.fontSize > 8) { // 防止字体太小\n        this.fontSize -= 2;\n      }\n    },\n    sureBtnClick() {\n      if (this.input.length <= 0) {\n        alert(\"请输入内容\");\n        return;\n      }\n      alert(this.input);\n      this.centerDialogVisible = false;\n    },\n    twoSum(nums, target) {\n      for (let i = 0; i < nums.length; i++) {\n        for (let j = 0; j < nums.length; j++) {\n          if (nums[i] + nums[j] === target) {\n            return [i, j]\n          }\n        }\n      }\n      return [-1, -1];\n    }\n  }\n};\n</script>\n\n<style scoped>\n.div_box {\n  padding: 20px 50px;\n  display: flex;\n  flex-direction: column;\n}\n</style>\n"],"mappings":"AA0BA;EACAA,IAAA,WAAAA,KAAA;IACA;MACAC,eAAA;MACAC,KAAA;MACAC,QAAA;MACAC,mBAAA;IACA;EACA;EACAC,OAAA;IACAC,gBAAA,WAAAA,iBAAA;MACA,KAAAH,QAAA;IACA;IACAI,gBAAA,WAAAA,iBAAA;MACA,SAAAJ,QAAA;QAAA;QACA,KAAAA,QAAA;MACA;IACA;IACAK,YAAA,WAAAA,aAAA;MACA,SAAAN,KAAA,CAAAO,MAAA;QACAC,KAAA;QACA;MACA;MACAA,KAAA,MAAAR,KAAA;MACA,KAAAE,mBAAA;IACA;IACAO,MAAA,WAAAA,OAAAC,IAAA,EAAAC,MAAA;MACA,SAAAC,CAAA,MAAAA,CAAA,GAAAF,IAAA,CAAAH,MAAA,EAAAK,CAAA;QACA,SAAAC,CAAA,MAAAA,CAAA,GAAAH,IAAA,CAAAH,MAAA,EAAAM,CAAA;UACA,IAAAH,IAAA,CAAAE,CAAA,IAAAF,IAAA,CAAAG,CAAA,MAAAF,MAAA;YACA,QAAAC,CAAA,EAAAC,CAAA;UACA;QACA;MACA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}